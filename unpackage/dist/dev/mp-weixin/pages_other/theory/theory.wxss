}
.page_big .page_content .page_bottom .bottom_btnbox .btnbox_right .success_exam {
  width: 100%;
  text-align: center;
  background: #8767f5;
  color: #ffffff;
  border-radius: 10rpx;
  padding: 10rpx 0 10rpx 0;
}

